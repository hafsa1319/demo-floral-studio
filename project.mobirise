          "_customHTML": "<section class=\"footer4\" group=\"Footer\" global=\"\" once=\"footers\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Logo Size\" name=\"logoSize\" min=\"2\" max=\"5\" step=\"1\" value=\"3\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#e0ecdc\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-md-6 col-lg-6 right\">\n                <div class=\"col-12 col-md-{{logoSize}} padd\">\n                    <img href=\"https://mobiri.se/\" src=\"@PROJECT_PATH@/assets/images/signature.png\" alt=\"Mobirise\">\n                </div>\n                <div mbr-form=\"\" class=\"mbr-form\">\n                    <form action=\"\" name=\"Form Name\" method=\"POST\" data-rcpha_sitekey=\"\" data-rcpha_secretkey=\"\" class=\"mbr-form form-with-styler\">\n                        <div class=\"form-row\">\n                            <div hidden=\"hidden\" data-form-alert=\"\" class=\"alert alert-success col-12\">Thanks for filling\n                                out the form!</div>\n                            <div hidden=\"hidden\" data-form-alert-danger=\"\" class=\"alert alert-danger col-12\">Oops...! some\n                                problem!</div>\n                        </div>\n                        <div class=\"dragArea form-row\">\n                            <div data-for=\"email\" class=\"form-group col-12 col-lg\">\n                                <input type=\"email\" name=\"email\" placeholder=\"Your Email\" data-form-field=\"email\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"\">\n                                    <div mbr-buttons=\"\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" class=\"col-auto button\">\n                                    <a type=\"submit\" class=\"btn btn-primary\" data-app-placeholder=\"Type Text\">Send</a>\n                                </div>\n                            </div>\n                        </div>\n                    </form>\n                </div>\n                <p class=\"mbr-text mbr-fonts-style copyright align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".copyright\" data-app-placeholder=\"Type Text\" mbr-if=\"showCopyright\">\n                     Copyright 2025 Mobirise - All Rights Reserved\n                </p>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-2 small\">\n                <h5 class=\"lin mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".lin\">\n                    <p class=\"bottom text-success\">About</p>\n                </h5>\n                <h5 class=\"small-bottom lin mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".lin\">\n                    <a href=\"index.html\" class=\"text-success it\">About Us<div class=\"line\"></div></a>\n                </h5>\n                <h5 class=\"small-bottom lin mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".lin\">\n                    <a href=\"index.html\" class=\"text-success it\">Our Partners<div class=\"line\"></div></a>\n                </h5>\n                <h5 class=\"lin mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".lin\">\n                    <a href=\"index.html\" class=\"text-success it\">Our Services<div class=\"line\"></div></a>\n                </h5>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-2 top small\">\n                <h5 class=\"lin mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".lin\">\n                    <p class=\"bottom text-success\">Contact</p>\n                </h5>\n                <h5 class=\"small-bottom lin mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".lin\">\n                    <a href=\"index.html\" class=\"text-success it\">Contact Us<div class=\"line\"></div></a>\n                </h5>\n                <h5 class=\"small-bottom lin mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".lin\">\n                    <a href=\"index.html\" class=\"text-success it\">FAQ Page<div class=\"line\"></div></a>\n                </h5>\n                <h5 class=\"lin mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".lin\">\n                    <a href=\"index.html\" class=\"text-success it\">About Me<div class=\"line\"></div></a>\n                </h5>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-2 top small\">\n                <h5 class=\"lin mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".lin\">\n                    <p class=\"bott